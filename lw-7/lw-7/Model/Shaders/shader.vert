#version 330 corelayout(location = 0) in vec3 aPosition;out float xPos;void main() {    float x = aPosition.x;    float R = (1.0 + sin(x)) *              (1.0 + 0.9 * cos(8.0 * x)) *              (1.0 + 0.1 * cos(24.0 * x)) *              (0.5 + 0.05 * cos(140.0 * x));    float xp = R * cos(x);    float yp = R * sin(x);    float zp = 0.0;    xPos = x;    gl_Position = vec4(xp * 0.3, yp * 0.3, zp, 1.0);    gl_PointSize = 1.0;}